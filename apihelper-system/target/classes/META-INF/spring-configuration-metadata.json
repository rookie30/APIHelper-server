{
  "groups": [
    {
      "name": "jwt",
      "type": "com.ning.modules.config.SecurityProperties",
      "sourceType": "com.ning.modules.config.ConfigBeanConfiguration",
      "sourceMethod": "securityProperties()"
    },
    {
      "name": "login",
      "type": "com.ning.modules.config.LoginProperties",
      "sourceType": "com.ning.modules.config.ConfigBeanConfiguration",
      "sourceMethod": "loginProperties()"
    }
  ],
  "properties": [
    {
      "name": "jwt.base64-secret",
      "type": "java.lang.String",
      "description": "必须使用最少88位的Base64对该令牌进行编码",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.code-key",
      "type": "java.lang.String",
      "description": "验证码 key",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.detect",
      "type": "java.lang.Long",
      "description": "token 续期检查",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.header",
      "type": "java.lang.String",
      "description": "Request Headers ： Authorization",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.online-key",
      "type": "java.lang.String",
      "description": "在线用户 key，根据 key 查询 redis 中在线用户的数据",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.renew",
      "type": "java.lang.Long",
      "description": "续期时间",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.token-start-with",
      "type": "java.lang.String",
      "description": "令牌前缀，最后留个空格 Bearer",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "jwt.token-validity-in-seconds",
      "type": "java.lang.Long",
      "description": "令牌过期时间 此处单位\/毫秒",
      "sourceType": "com.ning.modules.config.SecurityProperties"
    },
    {
      "name": "login.cache-enable",
      "type": "java.lang.Boolean",
      "description": "用户登录信息缓存",
      "sourceType": "com.ning.modules.config.LoginProperties",
      "defaultValue": false
    },
    {
      "name": "login.login-code",
      "type": "com.ning.modules.config.LoginCode",
      "sourceType": "com.ning.modules.config.LoginProperties"
    }
  ],
  "hints": []
}